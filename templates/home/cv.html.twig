<html lang="fr">
	<head>
		<!-- Required meta tags -->
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>CV</title>
		<!-- Bootstrap CSS -->
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.0/font/bootstrap-icons.css">
		<link href="{{ asset('css/style05.css') }}" rel="stylesheet">
		<link href="{{ asset('css/animate.css') }}" rel="stylesheet">
		<script src="{{ asset('js/style.js') }}" defer></script>
		<script src="{{ asset('js/wow.min.js') }}" defer></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.6.14/vue.min.js"></script>
		<!--â€ŠAdd Web Animations Polyfill :) -->
		<script src="https://cdnjs.cloudflare.com/ajax/libs/web-animations/2.3.2/web-animations.min.js"></script>
		<script type="module" src="https://cdn.jsdelivr.net/npm/@proyecto26/animatable-component@1.1.10/dist/animatable-component/animatable-component.esm.js" defer></script>
		<script src="https://kit.fontawesome.com/6672373333.js" crossorigin="anonymous"></script>
	</head>

	<body class="d-none">
		<section id="app" class="container-fluid">
			<div class="row">
				<header class="header position-relative">
					<animatable-component autoplay animation="fadeInDownBig" easing="ease-in-out" duration="1200">
						<div class="headLine position-absolute">
							<a href="{{ path('app_home')}}" class="snip1221 yellow ion-ios-home-outline">
								<i class="fa fa-home"></i>
								<span class="home">Accueil</span>
							</a>
						</div>
					</animatable-component>
					<div class="row d-flex flex-row col-sm-12">
						<div class="">
							{% for user in users %}
								<animatable-component autoplay animation="fadeInLeft" easing="ease-in-out" duration="1200">
									<h1 class="firstName text-nowrap">
										{{user.nom}}
									</h1>
								</animatable-component>
								<animatable-component autoplay animation="fadeInRight" easing="ease-in-out" duration="1200">
									<h2 class="lastName text-nowrap">
										{{user.prenom}}
									</h2>
								</animatable-component>
								<animatable-component autoplay animation="fade-in-left" easing="ease-in-out" duration="1200">
									<p class="jobTitle">
										<i class="bi bi-circle-fill"></i>
										<span class="text-break">
											Developpeur web
										</span>
									</p>
								</animatable-component>
							</div>
							<div>
								<div v-if="!!photo" class="col-auto d-none d-lg-block d-print-block">
									<animatable-component autoplay animation="rotateIn" easing="ease-in" duration="1200">
										<img src="{{ user.imagePath }}" class="photo border border-2 border-dark rounded-circle img-fluid float-end img-thumbnail">
									</animatable-component>
								</div>

								<animatable-component autoplay animation="fadeInUpBig" easing="ease-in-out" duration="1200">
									<div class="middleLine position-absolute"></div>
								</animatable-component>
							</div>
						</div>


					</header>
				</div>

				<section class="content">
					<div class="row">
						<div class="col personal-details me-4">
							<div class="row list-item me-5">
								<div class="col-auto">
									<animatable-component autoplay animation="heartBeat" easing="ease-in" duration="1200">
										<div class="icon d-flex">
											<i class="bi bi-geo-alt-fill" role="img" aria-label="Location"></i>
										</div>
									</animatable-component>
								</div>
								<div class="col-auto" style="white-space: pre-line;">
									<p class="wow bounceInDown">{{ user.adresse }}</p>
								</div>
							</div>
							<div v-if="!!phone" class="row list-item me-5">
								<div class="col-auto align-self-center">
									<animatable-component autoplay animation="swing" easing="ease-in" duration="1200">
										<div class="icon d-flex justify-content-center">
											<i class="bi bi-telephone-fill" role="img" aria-label="Telephone"></i>
										</div>
									</animatable-component>
								</div>
								<div class="col">
									<a v-bind:href="'tel:'+ phone" class="text-decoration-none">
										<p>{{ user.telephone }}</p>
									</a>
								</div>
							</div>
							<div class="row list-item me-5">
								<div class="col-auto align-self-start">
									<animatable-component autoplay animation="flip" easing="ease-in" duration="1200">
										<div class="icon d-flex justify-content-center">
											<i class="bi bi-envelope-fill" role="img" aria-label="Email"></i>
										</div>
									</animatable-component>
								</div>
								<div class="col">
									<a v-bind:href="'mailto:'+ email" class="text-decoration-none">
										<p>{{ user.email }}</p>
									</a>
								</div>
							{% endfor %}

						</div>
						<div class="row mt-5 me-5">
							<div class="col">
								<p class="title text-uppercase">Experiences<br/><span class="titre2">professionnelles</span>
								</p>

							</div>
						</div>
						<div class="row record">
							<div class="col-auto bullet"></div>
							{% for experience in experiences %}
								<div class="col-lg-12">
									<p class="fw-bold titre1 fs-4  text-uppercase">
										{{ experience.compagny }}

									</p>
									<p class="fw-bold titre1 fst-italic  text-lowercase">
										{{ experience.title }}
										<br/>
									</p>
									<p>

										<span class="titre1">{{ experience.startedAt | date('d/m/y') }}
											|
											{{ experience.endAt | date('d/m/y') }}</span>
									</p>
									<p>{{ experience.description }}</p><br/>
								</div><br/><br/>
							{% endfor %}
						</div>
						<div class="row">
							<div class="col p-0">
								<p class="title">Skills</p>
							</div>
						</div>
						<div class="row mt-5 me-5">
							<div class="col">
								{% for competence in competences %}
									<ul class="ms-2">
										<li class="pb-2 titre1">{{ competence.name }}</li>
									</ul>
								{% endfor %}
							</div>

						</div>
						<div class="row mt-5 me-5">
							<div class="col p-0">
								<p class="title">Langues</p>
							</div>
						</div>
						<div class="row">
							<div class="col">
								{% for langue in langues %}
									<ul class="ms-2">
										<li class="pb-2 titre1 ">{{ langue.name }}
											&nbsp;|&nbsp;
											{{ langue.level }}</li>
									</ul>
								{% endfor %}
							</div>
						</div>
						<!-- Loisirs -->
						<div class="row mt-5 me-5">
							<div class="col p-0">
								<p class="title">Loisirs</p>
							</div>
						</div>
						<div class="row">
							<div class="col">
								{% for loisir in loisirs %}
									<ul class="ms-2">
										<li class="pb-2 titre1">{{ loisir.name }}
										</li>
									</ul>
								{% endfor %}
							</div>
						</div>
						<!--- Fin de loisirs  -->
					</div>
					{% for user in users %}
						<div class="col-lg-8">
							<animatable-component autoplay animation="fade-in-right" easing="ease-in-out" duration="1200">
								<div class="row bullet">
									<div class="col-auto pt-2"></div>
									<div class="col">
										<p class="title">A propos de moi</p>
										<p>{{ user.aPropos }}</p>
									</div>
								{% endfor %}
							</div>
						</animatable-component>
						<animatable-component autoplay animation="fade-in-right" easing="ease-in-out" duration="1200"></animatable-component>

						<animatable-component autoplay animation="fade-in-top" easing="ease-in-out" duration="1200">
							<div class="row mt-5">
								<div class="col-auto bullet pt-3"></div>
								<div class="col">
									<p class="title text-uppercase">Parcours</p>
								</div>
							</div>
						</animatable-component>

						<div class="row record mb-4 bullet">

							<div class="col">
								{% for formation in formations %}

									<p class="text-uppercase titre1 fs-4 fw-bold position-relative">
										{{ formation.school }}
									</p>
									<p class="text-lowercase titre1 fst-italic fw-bold">
										{{ formation.level }}
										&nbsp;|&nbsp;
										<span class="fw-bold titre1">{{ formation.name }}</span>
									</p>
									<p>
										<span class="titre1">
											{{ formation.startedAt | date('d/m/y') }}
											&nbsp;|&nbsp;
											{{ formation.endAt | date('d/m/y') }}
										</span>
									</p>
									<p class="mb-0">
										{{ formation.description }}
									</p>
								</div>
							{% endfor %}
						</div>
					</div>
				</div>
			</section>
		</body>
	</body>
</html></section><div class="btn-group-fab d-print-none">
<div>
	<a href="{{ asset('CV.pdf')}}" target="_blank">
		<button type="button" class="btn btn-main btn-primary" title="Print">
			<i class="bi bi-printer fs-3"></i>
		</button>
	</a>
</div></div><!-- Bootstrap Bundle with Popper --><script src="{{ asset('js/wow.min.js') }}" defer></script><script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4" crossorigin="anonymous"></script></body></html></body></html></body></html></body></html> </body></body></html>
